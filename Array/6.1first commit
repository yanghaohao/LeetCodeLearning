public static int[] getNumber(int[] numbers,int target) {
		int[] index = {-1,1};
		int low = 0,high = numbers.length-1;
		while (low<high) {
			int sum = numbers[low] + numbers[high];
			if (sum == target) {
				index[0] = low+1;
				index[2] = high+1;
				return index;
			}else if (sum < target) {
				++low;
			}else {
				--high;
			}
		}
		
		return index;
	}
